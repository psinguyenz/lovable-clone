generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id    Int     @id @default(autoincrement())
  name  String?
}

model Post {
  id        Int     @id @default(autoincrement())
  content   String?
  published Boolean @default(false)
  authorId  Int
}